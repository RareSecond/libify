{
  "name": "@undo/backend",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:worker": "nest start --entryFile worker",
    "start:worker:dev": "nest start --watch --preserveWatchOutput --entryFile worker",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "kysely:generate": "prisma generate",
    "lint:check": "eslint . --max-warnings 0",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "cq:local": "npm run prettier && npm run lint && npm run typecheck"
  },
  "dependencies": {
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.0.10",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.10",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/platform-socket.io": "^11.1.7",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.7",
    "@prisma/client": "^6.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.15.4",
    "axios": "^1.8.2",
    "bullmq": "^5.0.0",
    "ioredis": "^5.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "joi": "^17.13.3",
    "kysely": "^0.28.3",
    "passport-jwt": "^4.0.1",
    "passport-spotify": "^2.0.0",
    "pg": "^8.16.3",
    "prisma-kysely": "^1.8.0",
    "reflect-metadata": "^0.2.2",
    "ts-pattern": "^5.6.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.3",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.0.10",
    "@types/cookie": "^0.6.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "prettier": "^3.5.1",
    "prisma": "^6.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
